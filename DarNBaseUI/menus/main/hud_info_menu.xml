		<onlyif src="me()" trait="user4" /> <!-- if telekenisis, else -->
		<add>
			<copy src="darnui_settings" trait="_ndred" /> <!-- normal red level -->
			<onlyifnot src="me()" trait="user4" /> <!-- if NOT telekenisis -->
		</add>
	</red>
	<green>
		<copy> 100 </copy> <!-- green level for telekenisis -->
		<onlyif src="me()" trait="user4" /> <!-- if telekenisis, else -->
		<add>
			<copy src="darnui_settings" trait="_ndgreen" /> <!-- normal green level -->
			<onlyifnot src="me()" trait="user4" /> <!-- if NOT telekenisis -->
		</add>
	</green>
	<blue>
		<copy> 100 </copy> <!-- blue level for telekenisis -->
		<onlyif src="me()" trait="user4" /> <!-- if telekenisis, else -->
		<add>
			<copy src="darnui_settings" trait="_ndblue" /> <!-- normal blue level -->
