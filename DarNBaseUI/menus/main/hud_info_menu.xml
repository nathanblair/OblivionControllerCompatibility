<!-- DarN Version 1.3 -->
<!-- hud_info_menu.xml -->
<menu name="HUDInfoMenu">
	<class> &HUDInfoMenu; </class>
	<stackingtype> &does_not_stack; </stackingtype>
	<visible> &false; </visible>
	<locus> &true; </locus>
	<depth> 20 </depth>
	<x> 0 </x>
	<y> 0 </y>
	
<!-- Settings -->
<_gzoom><copy src="sibling(HUDMainMenu)" trait="_zoom" /></_gzoom>
<_settings_mode><copy src="sibling(HUDMainMenu)" trait="_settingsmode" /></_settings_mode>
<_ha><copy src="sibling(HUDMainMenu)" trait="_gfx_hudalpha" /></_ha>
<_txt_ha><copy src="sibling(HUDMainMenu)" trait="_txt_hudalpha" /></_txt_ha>
<_3p><copy src="sibling(HUDReticle)" trait="_3p" /></_3p>
<!-- <_combat><copy src="sibling(HUDReticle)" trait="_combat" /></_combat> -->

<include src="darn\hud_info_menu_config.xml"/>	

	<_centerscreenx>
		<copy src="screen()" trait="width" />
		<div> 2 </div>
		<add src="darnui_settings" trait="_xoffset" />
		<add>
			<copy src="darnui_settings" trait="_3PXOffset" />
			<onlyif src="me()" trait="_3p" />
		</add>
	</_centerscreenx>

	<_centerscreeny>
		<copy src="screen()" trait="height" />
		<div> 2 </div>
		<add src="darnui_settings" trait="_yoffset" />
		<add>
			<copy src="darnui_settings" trait="_3PYOffset" />
			<onlyif src="me()" trait="_3p" />
		</add>
	</_centerscreeny>

	<menufade> 0.15 </menufade>
	<user0></user0>	<!-- Action text (set in code) -->
	<user1> 128 </user1> <!-- DarN used as visiblity flag -->
	<user2> &false; </user2> <!-- Visibility of A-button action text (xbox only) -->
	<user3> 20 </user3>
	<user4> &false; </user4> <!-- true if telekenisis is active -->
	<user5></user5>	<!-- Action button icon index (set in code, xbox only) -->
