				<files>
					<file source="UI_DarN_Dark/menus/options/video_menu.xml" destination="menus/options/" priority="2"/>
				</files>
			</pattern>
			<!--#################################################### Install MESSAGEBOX Menu ##########################################################-->
			<pattern> <!--################################################# Base DarN #################################################################-->
				<dependencies>
					<flagDependency flag="BaseDarNSelected" value="On"/>
					<flagDependency flag="MessageMenu" value="On"/>
				</dependencies>
				<files>
					<file source="UI_DarN_Base/menus/message_menu.xml" destination="menus/" priority="2"/>
				</files>
			</pattern>
			<pattern> <!--################################################### Dark DarN ###############################################################-->
				<dependencies>
					<flagDependency flag="DarkDarNSelected" value="On"/>
					<flagDependency flag="MessageMenu" value="On"/>
				</dependencies>
				<files>
					<file source="UI_DarN_Dark/menus/message_menu.xml" destination="menus/" priority="2"/>
				</files>
			</pattern>
			<!--####################################################### Install DIALOG Menu ###########################################################-->
			<pattern> <!--################################################# Base DarN #################################################################-->
				<dependencies>
					<flagDependency flag="BaseDarNSelected" value="On"/>
					<flagDependency flag="DialogMenu" value="On"/>
				</dependencies>
				<files>
					<file source="UI_DarN_Base/menus/dialog/dialog_menu.xml" destination="menus/dialog/" priority="2"/>
				</files>
			</pattern>
			<pattern> <!--################################################## Dark DarN #################################################################-->
				<dependencies>
					<flagDependency flag="DarkDarNSelected" value="On"/>
					<flagDependency flag="DialogMenu" value="On"/>
				</dependencies>
				<files>
					<file source="UI_DarN_Dark/menus/dialog/dialog_menu.xml" destination="menus/dialog/" priority="2"/>
				</files>
			</pattern>
		</patterns>
	</conditionalFileInstalls>
</config>