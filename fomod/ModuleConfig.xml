				</files>
			</pattern>
			<!--####################################################### Install LOAD Menu #############################################################-->
			<pattern> <!--################################################# Base DarN #################################################################-->
				<dependencies>
					<flagDependency flag="BaseDarNSelected" value="On"/>
					<flagDependency flag="LoadMenu" value="On"/>
				</dependencies>
				<files>
					<file source="UI_DarN_Base/menus/options/load_menu.xml" destination="menus/options/" priority="2"/>
				</files>
			</pattern>
			<pattern> <!--################################################ Dark DarN ##############################################################-->
				<dependencies>
					<flagDependency flag="DarkDarNSelected" value="On"/>
					<flagDependency flag="LoadMenu" value="On"/>
				</dependencies>
				<files>
					<file source="UI_DarN_Dark/menus/options/load_menu.xml" destination="menus/options/" priority="2"/>
				</files>
			</pattern>
			<!--####################################################### Install Audio Menu #############################################################-->
			<pattern> <!--################################################# Base DarN #################################################################-->
				<dependencies>
					<flagDependency flag="BaseDarNSelected" value="On"/>
					<flagDependency flag="AudioMenu" value="On"/>
				</dependencies>
				<files>
					<file source="UI_DarN_Base/menus/options/audio_menu.xml" destination="menus/options/" priority="2"/>
				</files>
			</pattern>
			<pattern> <!--################################################ Dark DarN ##############################################################-->
				<dependencies>
					<flagDependency flag="DarkDarNSelected" value="On"/>
					<flagDependency flag="AudioMenu" value="On"/>
				</dependencies>
				<files>
					<file source="UI_DarN_Dark/menus/options/audio_menu.xml" destination="menus/options/" priority="2"/>
				</files>
